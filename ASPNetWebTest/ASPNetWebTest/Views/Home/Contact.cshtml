<body>
    <div class="container-fluid needs" id="contactMain">
				
					<div class="row donor_header">
				
						<div class="col-xs-6 text">
						
							<h2 class="h2style1">CONTACT Juan if you seek men</h2>	
						
						</div>
				
					</div>
					
					
					<div class="row">
					
						<div class="col-xs-12 col-md-6 contact_form">
							
							<p class="contact_subheader">CONTACT US</p>
						
							<form name="myemailform" action="form-to-email.php" method="post">
							
								<input name="firstName" class="input_text" type="text" maxlength="20" placeholder="First Name"> <span class="change" id="fName_change">Required!</span>
								<br>
								<input name="lastName" class="input_text" type="text" maxlength="20" placeholder="Last Name"><span class="change" id="lName_change">Required!</span>
								<br>
								<input name="phone" type="tel" maxlength="20" placeholder="Telephone (Optional)">
								<br>
								<input name="email" class="input_text" type="email" maxlength="40" placeholder="Email Address"><span class="change" id="email_change">Required!</span>
								<br>
								<textarea name="message" class="input_text" maxlength="255" placeholder="Message"></textarea><span class="change" id="message_change">Required!</span>
								<br>
								<input disabled="" class="submit_button" id="submit" type="submit" value="Submit">
								<input class="cancel" type="reset" value="Cancel">	
								
							
							</form>
						
						</div>
						
						<div class="col-xs-12 col-md-6 two_col">
						
							<p class="contact_subheader other_info">CALL US</p>
							<p class="contact_info_text contact_tel">Tel: 801-528-1214</p>
							
							<p class="contact_subheader other_info">COME SEE US</p>
							<p class="contact_info_text">60 Adams Ave. Ogden, UT<br>Ogden, Utah 84401</p>
							
							<iframe class="map" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1784.5861387467835!2d-111.96804305268928!3d41.215141436084046!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x87530f2689623957%3A0x49def70f5ee0a151!2sOgden+Youth+Futures!5e0!3m2!1sen!2sus!4v1492809010928" allowfullscreen=""></iframe>						
							
						
						</div>
					
					</div>
					
					<div class="row contact_SM">
					
						<div class="col-xs-4">
				
							<p class="contact_subheader">SOCIAL MEDIA</p>
							
							<img title="instagram icon" alt="instagram icon" src="resources/contact_instagram.png">
							<img title="twitter icon" alt="twitter icon" src="resources/contact_twitter.png">
							<img title="tumblr icon" alt="tumblr icon" src="resources/contact_tumblr.png">
							<img title="fb icon" alt="fb icon" src="resources/contact_fb.png">
						
						</div>
					
					</div>
					
				
				</div>
</body>
</html>